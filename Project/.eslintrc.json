{
    "extends": "eslint:recommended",
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-console": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "env": {
        "browser": true,
        "node": true,
        "serviceworker": true,
        "es6": true,
        "jasmine": true
    },
    "ecmaFeatures": {
        "blockBindings": true,
        "modules": true,
        "superInFunctions": true
      }
}